{
  "name": "fullstack-bloglist-application",
  "version": "1.0.0",
  "description": "fullstack bloglist application",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "client": "cd ./client && react-scripts start",
    "server": "NODE_ENV=development nodemon index.js --ignore ./client",
    "build": "rm -rf build && cd ./client && react-scripts build && mv build ../ && cd .. && echo 'Frontend build complete'",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migration:down": "node ./server/utils/rollback.js"
  },
  "author": "Angelica Järvenpää",
  "license": "ISC",
  "dependencies": {
    "axios": "~0.27.2",
    "bcrypt": "~5.1.0",
    "cors": "~2.8.5",
    "dotenv": "~16.0.3",
    "express": "~4.18.2",
    "express-async-errors": "~3.1.1",
    "jsonwebtoken": "~8.5.1",
    "pg": "^8.11.3",
    "prop-types": "~15.8.1",
    "react": "~18.1.0",
    "react-dom": "~18.1.0",
    "react-scripts": "~5.0.1",
    "sequelize": "^6.33.0",
    "umzug": "^3.3.1",
    "web-vitals": "~2.1.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "concurrently": "~8.2.1",
    "cross-env": "~7.0.3",
    "nodemon": "~2.0.20",
    "supertest": "~6.3.2"
  },

  "exludes": [
    "node_modules"
  ]
}
